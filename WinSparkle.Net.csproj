<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netcoreapp2.0;netcoreapp2.1;netcoreapp2.2;netcoreapp3.0;net35;net40;net45</TargetFrameworks>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="WinSparkle32.dll" />
    <None Remove="WinSparkle64.dll" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="WinSparkle32.dll">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
    <EmbeddedResource Include="WinSparkle64.dll">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>

</Project>
